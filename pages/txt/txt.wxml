<block class="container">
<view class="title">申请表确认单</view>
<form bindsubmit="formSubmit">
  
  <view class="depar_title">部门/系：</view>
    <input class="depar" name="major_name" type="text" placeholder="识别失败，请手动输入" value="{{major_name}}"></input>
 <view class="driving_title">行驶证登记所有人：</view>
    <input class="driving" name="driving_name" type="text" placeholder="识别失败，请手动输入" value="{{driving_name}}"></input>
  <view class="driver_title">驾驶员：</view>
  <input class="driver" name="driver_name" type="text" placeholder="识别失败，请手动输入" value="{{driver_name}}"></input>
  <view class="telephone_title">手机号：</view>
    <input class="telephone" name="telephone" type="text" placeholder="识别失败，请手动输入" value="{{telephone}}"></input>
  <view class="car_title">车号：</view>
    <input class="car" name="car_number" type="text" placeholder="识别失败，请手动输入" value="{{car_number}}"></input>
  <button class="submit" formType="submit">确认提交</button>
</form>
</block>