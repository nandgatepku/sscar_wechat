<block class="container">
<view class="title">车证申请表</view>
<view>
   <image class="one" bindtap="up_driver_front"src="{{driver_front}}"></image>
   <image class="two" bindtap="up_driver_back" src="{{driver_back}}"></image>
   <image class="three" bindtap="up_driving_front"src="{{driving_front}}"></image>
   <image class="four" bindtap="up_driving_back" src="{{driving_back}}"></image>
   <image class="five" bindtap="up_studentcard" src="{{studentcard}}"></image>
   <image class="six" bindtap="up_car_front" src="{{car_front}}"></image>
</view>
<button class="totxt" type="default" bindtap="totxt">提交</button>
<view class="fin">
<view>{{obct}}</view>
<view>{{obcr}}</view>
<view wx:if="{{obcshow}}">
<view>身份证号：{{obcid}}</view>
<view>姓名：{{obcname}}</view>
<view>准驾车型：{{obccar}}</view>
<view>有效期至：{{obcend}}</view>
<view>住址：{{obcadd}}</view>
<view>发证机关：{{obcauth}}</view>
</view>
</view>
</block>